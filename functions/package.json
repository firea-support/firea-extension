      "firebase-functions": "^3.16.0",
      "axios":"^1.1.3"
    },
    "devDependencies": {
      "eslint": "^7.30.0",
      "eslint-config-google": "^0.14.0",
      "eslint-plugin-import": "^2.23.4",
      "firebase-functions-test": "^0.3.0",
      "rimraf": "^3.0.2"
    }
  }